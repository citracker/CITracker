                overlay.style.display = "none";
            }
        }
    </script>

    <script>
        function loadPartial(hide, show) {
            $("#"+hide).addClass("d-none")
            $("#"+show).removeClass("d-none")

            document.dispatchEvent(
                new CustomEvent('partialShown', {
                    detail: { id: show }
                })
            );
        }

        document.addEventListener('DOMContentLoaded', function () {

            // Show active child2 menus
            document.querySelectorAll('ul.slide-menu.child2.active').forEach(el => {
                el.style.display = 'block';
            });

            // Hide inactive child2 menus
            document.querySelectorAll('ul.slide-menu.child2:not(.active)').forEach(el => {
                el.style.removeProperty('display');
            });

        });
    </script>


