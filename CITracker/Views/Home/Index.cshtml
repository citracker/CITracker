                                    <rect x="13" y="13" width="10" height="10" fill="#FFD700"></rect>
                                    </svg>
                                    @Localizer["Sign in with SSO"]
                                </a>
                            }
                            else
                            {
                                if (hca?.HttpContext?.Session.GetString("OrganisationSubscriptionStatus")?.ToString() == "false")
                                {
                                    <a href="#pricing" class="btn btn-lg btn-w-lg btn-dark">
                                        @Localizer["Select a Subscription Plan"]
                                    </a>
                                }
                                else
                                {                                    
                                    <a href="@Url.Action("Dashboard", "Main")" class="btn btn-lg btn-w-lg btn-dark">
                                        @Localizer["Proceed to CITracker"]
                                    </a>
                                }
                            }
